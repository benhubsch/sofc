{
  "name": "react-node-starter",
  "version": "1.0.0",
  "description": "an example for deploying a React + NodeJS app to Heroku",
  "private": true,
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "backend": "nodemon server.js",
    "start": "npm run build && NODE_ENV=production nodemon server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@blueprintjs/core": "^2.3.1",
    "@blueprintjs/datetime": "^2.0.3",
    "@blueprintjs/select": "^2.0.1",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-transition-group": "^2.0.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "dom4": "^2.0.1",
    "enzyme": "^3.3.0",
    "expect": "^22.4.3",
    "express": "^4.16.3",
    "isemail": "^3.1.2",
    "lodash": "^4.17.10",
    "mathjs": "^4.2.2",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-datasheet": "^1.3.6",
    "react-dom": "^16.3.2",
    "react-popper": "^0.10.4",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^3.1.4",
    "ignore-styles": "^5.0.1",
    "nodemon": "^1.17.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2"
  }
}
